<div>
  <input class="form-control patient-search" placeholder="Search Patient" onkeyup="filterPatient(this.value)">
  <a class="patient-add-button" href='<%= new_patient_path %>' ><%= image_tag "add_user.png", :width=>30, :height => 30 %></a>
</div>
<div id="patientList">

<% @patients.each do |patient| %>
  <%= render 'patient_card', :patient => patient %>
    <% end %>
  </div>
